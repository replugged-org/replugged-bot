{
  "compilerOptions": {
    "declaration": false,
    "outDir": "out",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "noImplicitAny": true,
    "strict": true,
    "target": "ES2020",
    "useDefineForClassFields": true,
    "incremental": true,
    "sourceMap": true
  },
  "include": ["src"]
}
